{
    "compilerOptions": {
        "outDir": "dist", //编译好的文件输出到哪里
        "module": "esnext", //输出的模块的格式
        "target": "es5", //
        "declaration": true,
        "jsx": "react",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true
    },
    "include": ["src"],
    "exclude": ["src/**/*.test.tsx", "src/**/*.stories.tsx"]
}
